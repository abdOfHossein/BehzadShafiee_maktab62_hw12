    <div class="countainer">
        <div class="d-img"><img class="img" src="bgImg.webp" /></div>
        <div class="d-form">
            <form method="post" action="login">

                <label for="userName" class="form-label">userName</label>
                <input type="text" class="form-control" id="userName" aria-describedby="emailHelp"
                    placeholder="userName" name="username">



                <label for="pass" class="form-label">Password</label>
                <input type="password" class="form-control" id="pass" placeholder="password" name="password">


                <button id="btn-login" type="submit" class=" btn btn-primary">Login</button>
                <button id="btn-reg" type="button" class="btn btn-primary">Register(if you don't registered)</button>

            </form>
        </div>

    </div>
</body>

